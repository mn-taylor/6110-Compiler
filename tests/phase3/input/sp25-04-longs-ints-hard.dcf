import printf;

int get_int(int num) { return num; }
bool get_bool(bool bool_in) { return bool_in; }
long get_long(long num) { return num; }

void main () {
    int k;
    long l;
    int m;

    int n;
    long o;
    int p;

    // Go to the edge of INT.MAX_VALUE
    k = get_int(2147483647);
    // Add 100
    l = long(k) + get_long(100L);
    // Subtract 200
    m = int(l - get_long(200L));

    // Go to the edge of INT.MIN_VALUE
    n = get_int(-2147483648);
    // Subtract 100
    o = long(n) - get_long(100L);
    // Add 200
    p = int(o + get_long(200L));

    printf("%d, %d\n", m, p);
}