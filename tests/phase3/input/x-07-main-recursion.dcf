import printf;
import exit;
import strcmp;

int i;

void main_x() {
  i += 1;
  printf("enter main %d\n", i);
  if ( i < 10 ) {
    main_x();
  } else {
    return;
  }
  printf("exit main %d\n", i);
  i -= 1;

  if (i == 1) {
    exit(strcmp("AAA", "AAA") * printf("exit main 1\n"));
    main_x(); // Should not reach here
  }
}

void main(){
    i = 0;
    main_x();
    // Should not get here
    main();
}